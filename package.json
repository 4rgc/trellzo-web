{
  "name": "trellzo-web",
  "version": "0.6.2",
  "private": true,
  "dependencies": {
    "@hello-pangea/dnd": "^16.2.0",
    "@tanstack/react-query": "^4.24.9",
    "humanize-duration": "^3.28.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "sass": "^1.58.3",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:dev": "npm run test -- --watchAll --coverage",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-do": "docker build --platform=linux/amd64 -t registry.digitalocean.com/trellzo/trellzo-web:$npm_package_version --build-arg COMMIT_SHA=$(git rev-parse --short HEAD) --build-arg APP_VERSION=$npm_package_version .",
    "push-do": "docker push registry.digitalocean.com/trellzo/trellzo-web:$npm_package_version"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^7.0.0-beta.54",
    "@storybook/addon-essentials": "^7.0.0-beta.50",
    "@storybook/addon-links": "^7.0.0-beta.50",
    "@storybook/addon-storyshots": "^7.0.0-beta.50",
    "@storybook/node-logger": "^7.0.0-beta.50",
    "@storybook/preset-create-react-app": "^7.0.0-beta.50",
    "@storybook/react": "^7.0.0-beta.50",
    "@storybook/react-webpack5": "^7.0.0-beta.50",
    "@storybook/testing-react": "^2.0.0-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/enzyme": "^3.10.12",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^18.0.11",
    "@types/react-table": "^7.7.14",
    "babel-jest": "^29.4.3",
    "enzyme": "^3.11.0",
    "fetch-mock": "^9.11.0",
    "mini-css-extract-plugin": "^2.7.2",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "storybook": "^7.0.0-beta.50",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/.*.stories.tsx"
    ]
  }
}
